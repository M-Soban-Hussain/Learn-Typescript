// Define 'Author' type with first and last names.
type Author = {
    firstName: string;
    lastName: string;
};

// Define 'Book' type with author (of 'Author' type) and name.
type Book = {
    author: Author;
    name: string;
};

// Create 'myBook' object of type 'Book' with author and name.
let myBook: Book = {
    author: {
        firstName: "Zia",
        lastName: "Khan"
    },
    name: "My Best Book"
};
